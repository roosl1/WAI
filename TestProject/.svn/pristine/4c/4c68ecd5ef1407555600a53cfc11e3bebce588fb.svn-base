@charset "EUC-KR";
#all-search{
	height:40px;
	width:230px;
	border: 2px solid #000000;
	background: #ffffff;
}
#all-search input{
	font-size: 16px;
	width: 150px;
	padding: 10px;
	border: 0px;
	outline: none;
	float: left;
}
#all-search button{
	width: 50px;
	height: 100%;
	border: 0px;
	background: #000000;
	outline: none;
	float: right;
	color: #ffffff
}